  rankdir=LR;
  orientation=portrait;
  fontsize=8;
  start [shape=box];
  start -> state016;
  state000 [label="[s]",shape=box];
    state000 -> state004 [style=bold];
  state002 [label="[ !\"#$%&'()*+,-./0123456789:;<=>?@ABCDEFGHIJKLMNOPQRSTUVWXYZ[\]^_`abcdefghijklmnopqrstuvwxyz{|}~]",shape=box];
